all_func_together1 <- function(data, pos_X, pos_y, method){
  data <- data_selection(data, pos_X, pos_y, method)
  order = data_into_five(data)  
  llm(data, order)
  lss0(data, order)
  ridgee(data, order)
  tree_stat(data)
  rf_cv(data)
}
all_func_together1(data, c(9:14, 7), c(8), 'mean')

